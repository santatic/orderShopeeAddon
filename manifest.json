{
    "name": "My First Chrome Ext",
    "description": "Firestore",
    "version": "1.0",
    "options_page": "options.html",

    "browser_action": {
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        },
        "default_popup": "popup/popup.html",
        "default_title": "Add order to my firestore"
    },
    "background": {
        "page": "background.html"
    },
    "content_scripts": [{
        "matches": [
            "https://banhang.shopee.vn/*",
            "https://shopee.vn/*", 
            "https://detail.1688.com/*"
        ],
        "js": [
            "lib/jquery.min.js",
            
           

            //"jquery-3.1.1.min.js", 
            "lib/bootstrap/js/bootstrap.min.js", 
            "lib/angular/angular.min.js",
            "lib/moment/moment.js",
            "lib/moment/angular-moment.min.js",
            "lib/moment/vi.js",
            "lib/jszip/jszip.min.js",
            "lib/jszip/jszip-utils.min.js",
            "lib/jszip/FileSaver.min.js",
            "lib/noty/js/noty.min.js",
            //"content.js" 
            "lib/qrcode.min.js",
            "js/content.js"           
        ],
        "css":["lib/bootstrap/css/bootstrap.min.css", "lib/noty/css/noty.css", "lib/custom/css/custom.css"],
        "run_at": "document_end"//document_start
