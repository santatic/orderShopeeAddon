{
    "name": "My First Chrome Ext",
    "description": "Firestore",
    "version": "1.0",
    "browser_action": {
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        },
        "default_popup": "popup/popup.html",
        "default_title": "Add order to my firestore"
    },
    "background": {
        "page": "background.html"
    },
    "content_scripts": [{
        "matches": [
            "https://banhang.shopee.vn/*"
        ],
        "css":["css/custom.css"],
        "js": [
            "lib/jquery.min.js",
            "lib/qrcode.min.js",
            "js/content.js"
        ],
        "run_at": "document_start"
    }],

    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "debugger",
        "identity",
        "webRequest",
        "webRequestBlocking",
        "*://banhang.shopee.vn/",
        "*://shopee.vn/",
        "*://cf.shopee.vn/"
    ],
    "content_security_policy": "script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs=' https://www.gstatic.com https://*.firebaseio.com; object-src 'self'",
    "manifest_version": 2
}